{
	"semi": false,
	"trailingComma": "es5",
	"singleQuote": true,
	"printWidth": 180,
	"tabWidth": 2,
	"useTabs": true,
	"bracketSpacing": true,
	"arrowParens": "avoid",
	"endOfLine": "lf",
	"jsxSingleQuote": false,
	"quoteProps": "as-needed",
	"proseWrap": "always",
	"htmlWhitespaceSensitivity": "css",
	"plugins": [
		"prettier-plugin-svelte"
	],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
